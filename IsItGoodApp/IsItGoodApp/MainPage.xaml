<Page
    x:Class="IsItGoodApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IsItGoodApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Width="600" Height="600">

    <StackPanel Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <CommandBar>
            <CommandBar.Content>
                <TextBlock x:Name="pageName" Margin="75,13,0,0" Text="Search"></TextBlock>
            </CommandBar.Content>
			<AppBarButton Icon="Next" Label="TestLandingPage" Click="AppBarButton_Click" />
			<CommandBar.SecondaryCommands>
                <AppBarButton Icon="ContactInfo" Label="About" />
                <AppBarButton Icon="Back" Label="Exit" />
            </CommandBar.SecondaryCommands>
        </CommandBar>
        <TextBox x:Name="tbSearch" Header="Search:" AcceptsReturn="False" Margin="100,80,100,10" />
        <TextBox x:Name="tbLocation" Header="Location:" AcceptsReturn="False" Margin="100,30,100,10"/>
        <Button x:Name="btnSearch" Content="Search" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="170,30,170,10" Click="btnSearch_Click"/>
    </StackPanel>
</Page>
